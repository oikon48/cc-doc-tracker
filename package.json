{
  "name": "cc-doc-tracker",
  "version": "1.0.0",
  "description": "Automatically fetch and track Claude Code documentation changes using Git-based scraping. Pure Markdown storage without HTML processing.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "fetch-docs": "tsx src/fetch-docs.ts",
    "test": "echo \"Test placeholder - to be implemented\"",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "claude-code",
    "claude",
    "anthropic",
    "documentation",
    "doc-tracker",
    "scraper",
    "markdown",
    "git-scraping",
    "automation",
    "typescript",
    "github-actions",
    "change-tracking"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.3.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}